{
  "name": "ng2-runtime",
  "description": "Angular2 JSON Schema Based Runtime",
  "scripts": {
    "test": "tsc && karma start",
    "test-watch": "tsc && karma start --no-single-run --auto-watch",
    "prepublish": "typings install && tsc && node make.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akserg/ng2-runtime.git"
  },
  "keywords": [
    "angular2",
    "json schema",
    "runtime"
  ],
  "author": "Sergey Akopkokhyants",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akserg/ng2-runtime/issues"
  },
  "main": "ng2-runtime.js",
  "typings": "./ng2-runtime.d.ts",
  "homepage": "https://github.com/akserg/ng2-runtime",
  "peerDependencies": {
    "angular2": "~2.0.0-beta.7",
    "es6-promise": "~3.0.2",
    "es6-shim": "~0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "~0.5.10"
  },
  "devDependencies": {
    "angular2": "2.0.0-beta.7",
    "cz-conventional-changelog": "^1.1.5",
    "es6-promise": "~3.0.2",
    "es6-shim": "~0.33.3",
    "jasmine-core": "~2.3.4",
    "karma": "~0.13.15",
    "karma-chrome-launcher": "~0.2.2",
    "karma-firefox-launcher": "~0.1.7",
    "karma-jasmine": "~0.3.6",
    "karma-typescript-preprocessor": "0.0.21",
    "reflect-metadata": "0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "semantic-release": "^4.3.5",
    "systemjs": "~0.19.6",
    "systemjs-builder": "^0.15.6",
    "tsd": "^0.6.5",
    "typescript": "~1.7.3",
    "typings": "^0.6.8",
    "zone.js": "^0.5.15"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
